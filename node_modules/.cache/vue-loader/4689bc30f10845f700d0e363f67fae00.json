ators=\"true\"\n    >\n      <swipe-item>\n        <grid>\n          <grid-item v-for=\"(item, index) in swiperList1\" :key=\"index\">\n            <Van-Image\n              width=\"0.84rem\"\n              hidden=\"0.84rem\"\n              :src=\"'https://fuss10.elemecdn.com/' + item.image_url\"\n            />\n            <span>{{ item.title }}</span>\n          </grid-item>\n        </grid>\n      </swipe-item>\n      <swipe-item>\n        <grid>\n          <grid-item v-for=\"(item, index) in swiperList2\" :key=\"index\">\n            <Van-Image\n              width=\"0.84rem\"\n              hidden=\"0.84rem\"\n              :src=\"'https://fuss10.elemecdn.com/' + item.image_url\"\n            />\n            <span>{{ item.title }}</span>\n          </grid-item>\n        </grid>\n      </swipe-item>\n    </swipe>\n\n    <!-- 展示页 -->\n\n    <ShopList :shopListArr=\"shopListArr\"></ShopList>\n\n    <!-- 下 导航栏 -->\n\n    <!-- <van-tabbar v-model=\"active\" class=\"van-tabbar\" fixed=\"true\">\n      <van-tabbar-item icon=\"home-o\">\n        <span>首页</span>\n      </van-tabbar-item>\n      <van-tabbar-item icon=\"search\">发现</van-tabbar-item>\n      <van-tabbar-item icon=\"friends-o\">订单</van-tabbar-item>\n      <van-tabbar-item icon=\"setting-o\">我的</van-tabbar-item>\n    </van-tabbar>-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from \"@/components/HelloWorld.vue\";\nimport { getPosiDate, getIndexEntryDate, getRestaurantsDate } from \"@/api/data\";\nimport {\n  Image,\n  NavBar,\n  Icon,\n  Swipe,\n  SwipeItem,\n  Grid,\n  GridItem,\n  Tabbar,\n  TabbarItem,\n  ShowList,\n} from \"vant\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      titlePosition: \"\",\n      swiperList1: [],\n      swiperList2: [],\n      showListArr: [],\n    };\n  },\n  components: {\n    NavBar,\n    Icon,\n    Swipe,\n    SwipeItem,\n    Grid,\n    GridItem,\n    VanImage: Image,\n    Tabbar,\n    TabbarItem,\n  },\n  method: {\n    onClickLeft() {\n      Toast(\"返回\");\n    },\n    onClickRight() {\n      Toast(\"按钮\");\n    },\n  },\n  async mounted() {\n    getPosiDate().then((res) => {\n      this.titlePosition = res.name;\n      console.log(res);\n    });\n    getIndexEntryDate().then((res) => {\n      this.swiperList1 = res.slice(0, 8);\n      this.swiperList2 = res.slice(8, 16);\n      console.log(res);\n    });\n    getRestaurantsDate().then((res) => {\n      this.showListArr = res;\n      console.log(res);\n    });\n  },\n};\n</script>\n\n<style lang=\"less\">\n#home {\n  .van-nav-bar {\n    height: 1rem;\n    background: linear-gradient(90deg, #0af, #0085ff);\n    .iconfont {\n      font-size: 0.48rem;\n    }\n  }\n  .van-nav-bar,\n  .van-nav-bar__title {\n    color: #fff;\n    font-size: 0.36rem;\n  }\n  .my-swipe {\n    height: 3.6rem;\n  }\n  .van-tabbar {\n    height: 1rem;\n  }\n}\n</style>\n"]}]}